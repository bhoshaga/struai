{
  "name": "struai",
  "version": "1.1.0",
  "description": "StruAI Drawing Analysis SDK - AI-powered construction drawing analysis",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "struai",
    "structural-engineering",
    "construction-drawings",
    "drawing-analysis",
    "cad",
    "pdf",
    "ai",
    "knowledge-graph"
  ],
  "author": "StruAI <support@struai.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhoshaga/struai.git",
    "directory": "js"
  },
  "homepage": "https://struai.com",
  "bugs": {
    "url": "https://github.com/bhoshaga/struai/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
